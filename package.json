  },
  "dependencies": {
    "underscore": "1.6.0",
    "moment": "2.5.1",
    "socket.io": "0.9.16",
    "nbt": "0.3.0",
    "bluebird": "1.1.1",
    "mkdirp": "0.3.5",
    "express": "3.5.0",
    "page": "1.3.7",
    "jquery": "2.1.0",
    "backbone": "1.1.2",
    "minimist": "0.0.8",
    "convict": "0.4.2",
    "properties-parser": "0.1.1",
    "ractive": "^0.4.0-pre2"
  },
  "devDependencies": {
    "fez": "1.0.8",
    "browserify": "3.32.0",
    "through": "2.3.4",
    "less": "1.7.0",
    "module-deps": "1.8.0",
    "coffeeify": "0.6.0",
    "browser-builtins": "3.1.0",

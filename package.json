    "bluebird": "2.3.2",
    "compression": "^1.0.11",
    "convict": "0.4.2",
    "express": "4.8.7",
    "jquery": "2.1.1",
    "less-middleware": "1.0.4",
    "minimist": "1.1.0",
    "mkdirp": "0.5.0",
    "moment": "2.8.2",
    "morgan": "^1.2.3",
    "nbt": "0.3.0",
    "page": "1.3.7",
    "properties-parser": "0.2.3",
    "ractive": "0.4.0-pre2",
    "serve-static": "^1.5.3",
    "socket.io": "1.0.6",
    "underscore": "1.7.0"
  },
  "devDependencies": {
    "fez": "1.0.8",
    "browserify": "5.11.0",
    "through": "2.3.4",
    "module-deps": "3.5.5",

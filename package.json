{
  "name": "crafty",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/appleifreak/crafty.git"
  },
  "scripts": {
    "start": "./bin/crafty -p 80 > out.log 2>&1 &",
